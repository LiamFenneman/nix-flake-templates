{
    "server": {
        "host": "127.0.0.1:3000"
    },

    "routes": {
        "/": "index.html"
    },

    "static": {
        "directory": "assets",
        "served_from": "/"
    },

    "config": {
        "enable_hot_reload": true,
        "fast_mem_cache": true,
        "enable_cache_control": true,
        "enable_directory_listing": true,
        "minify_html": false,
        "follow_symlinks": false,
        "enable_logging": false
    }
}
